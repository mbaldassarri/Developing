<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <corners
        android:bottomLeftRadius="50dp"
        android:bottomRightRadius="50dp"
        android:topLeftRadius="50dp"
        android:topRightRadius="50dp" />
    <gradient
        android:angle="45"
        android:centerColor="#FFFF0D"
        android:centerX="35%"
        android:endColor="#FF4545"
        android:startColor="#E8E8E8"
        android:type="linear" />
    <padding
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp" />
    <size
        android:width="50dp"
        android:height="50dp" />
    <stroke
        android:width="3dp"
        android:color="#335945" />
</shape>